

<form action="/dashboard/create-partner" method="POST" class="col-8 border p-3 form-dashboard mx-auto" enctype="multipart/form-data">
  <div class="form-group d-flex">
    <div class="fifty">
      <label>Nom</label>
      <input type="text" class="made-form" placeholder="Conseil régionale d'Île-de-France..." name="name">
    </div >
    <div class="fifty">
      <label>Site</label>
      <input type="text" class="made-form" placeholder="https:// ..." name="link">
    </div>
  </div>

  <div class="form-group d-flex">
    <div class="fifty">
      <label>Photo principale</label><br/>
      <input type="file" name="image"/>
    </div>  
  </div>

  <div class="hundred text-center mt-3">
    <button class="btn btn-warning action-button text-white"> Créer</button>
  </div>

</form>


